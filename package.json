    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.1.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/runtime": "^7.18",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
