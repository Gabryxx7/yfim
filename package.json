{
  "name": "webrtc-video-room",
  "version": "2.0.3",
  "type": "module",
  "description": "WebRTC Video Room is a Single-Page App in React using React Router that provides peer-to-peer video and audio communication in a web browser with minimum dependencies required.",
  "scripts": {
    "start": "node server.js",
    "build_start": "node ./node_modules/.bin/webpack --config webpack.prod-config.cjs && node server.js",
    "dev": "node ./node_modules/.bin/webpack --config webpack.dev-config.cjs",
    "start_win_dev": "node ./node_modules/webpack/bin/webpack.js --mode development",
    "start_win": "node ./node_modules/webpack/bin/webpack.js build && node server.js",
    "start_bak_win": "node --openssl-legacy-provider ./node_modules/webpack/bin/webpack.js build && node server.js",
    "start_bak": "node --openssl-legacy-provider ./node_modules/.bin/webpack build && node server.js",
    "serverstart": "npx webpack && node server.js",
    "build": "webpack --config webpack.prod-config.cjs ",
    "postinstall": "webpack --config webpack.prod-config.cjs ",
    "watch": "webpack --watch",
    "test-session-manager": "node test_sessions.js"
  },
  "browser": {
    "fs": false
  },
  "keywords": [
    "WebRTC Video Chat",
    "RTCPeerConnection",
    "React"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@babel/preset-env": "^7.22.6",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "css-loader": "^6.8.1",
